{
		"tags": {
				"allowUnknownTags": true,
				"dictionaries": [
					"jsdoc",
					"closure"
				]
		},
		"source": {
			"include": [
				"./build",
				"./package.json",
				"README.md"
			]
		},
		"plugins": [],
		"templates": {
				"cleverLinks": false,
				"monospaceLinks": false,
				"default": {
					"includeDate": false
				}
		},
		"opts": {
			"destination": "./doc",
			"recurse": true,
			"tutorials": "./tutorials"
		}
}
